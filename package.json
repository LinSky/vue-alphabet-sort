{
  "name": "vue-alphabet-sort",
  "version": "0.1.1",
  "private": false,
  "main": "lib/alphabet-sort-list.umd.js",
  "license": "MIT",
  "author": "sanmu",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name alphabet-sort-list --dest lib packages/index.js"
  },
  "dependencies": {
    "better-scroll": "^1.13.2",
    "lib-flexible": "^0.3.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "postcss-plugin-px2rem": "^0.8.0",
    "vue-template-compiler": "^2.5.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LinSky/vue-alphabet-sort.git"
  },
  "keywords": [
    "vue",
    "select",
    "sort",
    "alphabet",
    "pinyin",
    "first alphabet"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-plugin-px2rem": {
        "rootValue": 75
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
